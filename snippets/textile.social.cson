'.text.html.textile':
  'GitHub followers':
    'prefix': 'svg-github-followers'
    'body': '"!https://img.shields.io/github/followers/${1:user}/${2:repository}.svg${3:?style=flat${4:-square}}(GitHub)!":${5:https://github.com/${1:user}/${2:repository}}'
  'GitHub forks':
    'prefix': 'svg-github-forks'
    'body': '"!https://img.shields.io/github/forks/${1:user}/${2:repository}.svg${3:?style=flat${4:-square}}(GitHub)!":${5:https://github.com/${1:user}/${2:repository}}'
  'GitHub stars':
    'prefix': 'svg-github-stars'
    'body': '"!https://img.shields.io/github/stars/${1:user}/${2:repository}.svg${3:?style=flat${4:-square}}(GitHub)!":${5:https://github.com/${1:user}/${2:repository}}'
  'Twitter follow':
    'prefix': 'svg-twitter-follow'
    'body': '"!https://img.shields.io/badge/https://img.shields.io/badge/Follow-%40${1:username}-${2:blue}.svg${3:&style=flat${4:-square}}(Twitter)!":http://twitter.com/${1:username}'
  'Twitter URL':
    'prefix': 'svg-twitter-url'
    'body': '"!https://img.shields.io/twitter/url/https/${1:url}.svg${3:?style=flat${4:-square}}(Twitter)!":$5'
