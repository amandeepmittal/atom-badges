'.source.gfm':
  'apm: version':
    'prefix': 'png-apm-ver'
    'body': '[![apm](https://img.shields.io/apm/v/${1:package}.png${2:?style=flat${3:-square}})](${4:https://atom.io/${5:package}/${1:package}})'
  'Bower: version':
    'prefix': 'png-bower-ver'
    'body': '[![Bower](https://img.shields.io/bower/v/${1:package}.png${2:?style=flat${3:-square}})]($4)'
  'Chef Cookbook: version':
    'prefix': 'png-chef-ver'
    'body': '[![Chef Cookbook](https://img.shields.io/cookbook/v/${1:cookbook}.png${2:?style=flat${3:-square}})](${4:https://supermarket.chef.io/cookbooks/${1:cookbook}})'
  'Chocolatey: version':
    'prefix': 'png-chocolatey-ver'
    'body': '[![Chocolatey](https://img.shields.io/chocolatey/v/${1:package}.png${2:?style=flat${3:-square}})]($4)'
  'CocoaPods: version':
    'prefix': 'png-cocoapods-ver'
    'body': '[![CocoaPods](https://img.shields.io/cocoapods/v/${1:pod}.png${2:?style=flat${3:-square}})]($4)'
  'Gem: version':
    'prefix': 'png-gem-ver'
    'body': '[![Gem](https://img.shields.io/gem/v/${1:gem}.png${2:?style=flat${3:-square}})]($4)'
