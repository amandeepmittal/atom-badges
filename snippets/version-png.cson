    'prefix': 'png-github-forks'
    'body': '[![GitHub forks](https://img.shields.io/github/forks/${1:user}/${2:repository}.png${3:?style=flat${4:-square}})](${5:https://github.com/${1:user}/${2:repository}/network})'
  'GitHub: version':
    'prefix': 'png-github-release'
    'body': '[![GitHub release](https://img.shields.io/github/release/${1:user}/${2:repository}.png${3:?style=flat${4:-square}})](${5:https://github.com/${1:user}/${2:repository}/releases})'
  'GitHub: tag':
    'prefix': 'png-github-tag'
    'body': '[![GitHub tag](https://img.shields.io/github/tag/${1:user}/${2:repository}.png${3:?style=flat${4:-square}})](${5:https://github.com/${1:user}/${2:repository}/tags})'
  'Hackage: version':
    'prefix': 'png-hackage-ver'
    'body': '[![Hackage](https://img.shields.io/hackage/v/${1:package}.png${2:?style=flat${3:-square}})](${4:https://hackage.haskell.org/package/${1:package}})'
  'Hex.pm: version':
