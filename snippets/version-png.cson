  'NuGet: version':
    'prefix': 'png-nuget-ver'
    'body': '[![NuGet](https://img.shields.io/nuget/v/${1:package}.png${2:?style=flat${3:-square}})](${4:http://www.nuget.org/packages/${1:package}})'
  'Packagist: version':
