'.text.restructuredtext':
  'GitHub followers':
    'prefix': 'svg-github-followers'
    'body': """
.. image:: https://img.shields.io/github/followers/${1:user}/${2:repository}.svg${3:?style=flat${4:-square}}
   :target: ${5:https://github.com/${1:user}/${2:repository}}
    """
  'GitHub forks':
    'prefix': 'svg-github-forks'
    'body': """
.. image:: https://img.shields.io/github/forks/${1:user}/${2:repository}.svg${3:?style=flat${4:-square}}
   :target: ${5:https://github.com/${1:user}/${2:repository}}
    """
  'GitHub stars':
    'prefix': 'svg-github-stars'
    'body': """
.. image:: https://img.shields.io/github/stars/${1:user}/${2:repository}.svg${3:?style=flat${4:-square}}
   :target: ${5:https://github.com/${1:user}/${2:repository}}
    """
  'Twitter follow':
    'prefix': 'svg-twitter-follow'
    'body': """
.. image:: https://img.shields.io/badge/https://img.shields.io/badge/Follow-%40${1:username}-${2:blue}.svg${3:&style=flat${4:-square}}
   :target: http://twitter.com/${1:username}
    """
  'Twitter URL':
    'prefix': 'svg-twitter-url'
    'body': """
.. image:: https://img.shields.io/twitter/url/https/${1:url}.svg${3:?style=flat${4:-square}}
   :target: $5'
    """
